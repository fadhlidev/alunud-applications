    "/api/zakatfitrah": {
      "get": {
        "summary": "Get Zakat Fitrah Edition List",
        "responses": {
          "200": {
            "description": "Success get zakat fitrah edition list",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "number",
                      "example": 200
                    },
                    "message": {
                      "type": "string",
                      "example": "SUCCESS_GET_ZAKAT_FITRAH_EDITION_LIST"
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/ZakatFitrahEdition"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create Zakat Fitrah Edition",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "year": {
                    "type": "integer",
                    "nullable": false,
                    "example": 2023
                  },
                  "startDate": {
                    "type": "integer",
                    "nullable": false,
                    "example": 1681578000000
                  },
                  "amountPerPerson": {
                    "type": "integer",
                    "nullable": false,
                    "minimum": 2.5,
                    "example": 2.5,
                    "default": 2.5
                  }
                },
                "required": [
                  "year",
                  "startDate",
                  "amountPerPerson"
                ]
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Success create zakat fitrah edition",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "number",
                      "example": 201
                    },
                    "message": {
                      "type": "string",
                      "example": "SUCCESS_CREATE_ZAKAT_FITRAH_EDITION"
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "allOf": [
                          {
                            "$ref": "#/components/schemas/ZakatFitrahEdition"
                          },
                          {
                            "type": "object",
                            "properties": {
                              "endDate": {
                                "type": "integer",
                                "nullable": true,
                                "example": null
                              }
                            }
                          }
                        ]
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
